<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TrangHomeBacSiActivity"
    android:id="@+id/google_pixe"
    android:clipToOutline="true"
    android:background="#FFFFFF">
    <View
        android:id="@+id/rectangle_1"
        android:layout_width="414dp"
        android:layout_height="124dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="-1dp"
        android:layout_alignParentTop="true"
        android:background="#DF1B1B"
        />


    <View
        android:id="@+id/image_3"
        android:layout_width="148dp"
        android:layout_height="98dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="-13dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="36dp"
        android:background="@drawable/img"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="140dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/imgBack3"
            android:layout_width="26.3dp"
            android:layout_height="25.82dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="0dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/img_10" />

        <TextView
            android:id="@+id/textView10"
            android:layout_marginTop="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="25dp"
            android:text="Trở về trang đăng nhập" />
    </LinearLayout>



    <View
        android:id="@+id/rectangle_9"
        android:layout_width="373dp"
        android:layout_height="108dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="17dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="290dp"
        android:background="@color/daquang"
        />
    <View
        android:id="@+id/image_6"
        android:layout_width="107dp"
        android:layout_height="92dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="28dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="298dp"
        android:background="@drawable/img_3"
        />
    <Button
        android:id="@+id/h_s_b_nh_nh"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="150dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="320dp"
        android:text="Hồ sơ bệnh nhân"
        android:background="@color/daquang"
        />



    <View
        android:id="@+id/rectangle_6"
        android:layout_width="373dp"
        android:layout_height="107dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="17dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="530dp"
        android:background="@color/ghi"
        />
    <Button
        android:id="@+id/image_8"
        android:layout_width="107dp"
        android:layout_height="77dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="28dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="546dp"
        android:background="@drawable/img_5"
        />
    <Button
        android:id="@+id/n_thu_c"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="150dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="560dp"
        android:text="Đơn thuốc"
        android:background="@color/ghi"
        />
    <View
        android:id="@+id/line_1"
        android:layout_width="414dp"
        android:layout_height="0dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="-3dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="650dp"
        android:background="@color/black"
        />






</RelativeLayout>
